{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "ecmaFeatures": {
    "generators": true,
    "arrowFunctions": true
  },
  "rules": {
    "no-new-object": 2,
    "no-array-constructor": 2,
    "quotes": [
      2,
      "single"
    ],
    "indent": [
      2,
      2
    ],
    "max-len": [2, 80, 2],
    "consistent-return": 0,
    "curly": [2, "multi-line"],
    "no-multi-str": 2,
    "no-multi-spaces": 2,
    "no-shadow-restricted-names": 2,
    "no-shadow": 0,
    "no-underscore-dangle": 0,
    "dot-notation": 2,
    "block-scoped-var": 2,
    "vars-on-top": 0,
    "no-unused-vars": 2,
    "eqeqeq": 2,
    "use-isnan": 2,
    "no-eq-null": 2,
    "brace-style": 2,
    "spaced-line-comment": 2,
    "valid-jsdoc": 2,
    "space-infix-ops": 2,
    "eol-last": 2,
    "no-console": 1,
    "no-debugger": 2,
    "valid-jsdoc": 2,
    "no-alert": 2,
    "padded-blocks": [
      2,
      "never"
    ],
    "space-before-blocks": 2,
    "comma-dangle": [
      2,
      "never"
    ],
    "comma-style": 2,
    "semi": [
      2,
      "always"
    ],
    "semi-spacing": 2,
    "no-extra-boolean-cast": 2,
    "space-before-function-paren": [
      2,
      "never"
    ],
    "no-spaced-func": 2,
    "no-wrap-func": 2,
    "one-var": [
      2,
      "never"
    ],
    "camelcase": 2,
    "new-cap": 2,
    "key-spacing": 2,
    "no-dupe-keys": 2,
    "strict": 0
  }
}
